{
    "extends": ["tslint:latest", "tslint-react"],
    "rules": {
        "no-console": false,
        "max-line-length": [true, {
            "limit": 135,
            "ignore-pattern": "^[\t\\s]*(.+)$"
        }],
        "jsx-no-multiline-js": false,
        "no-empty": [true, "allow-empty-catch"],
        "arrow-parens": false
    }
}
